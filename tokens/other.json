{
  "transition": {
    "base": {
      "$value": "200ms",
      "$type": "duration"
    }
  },
  "shadow": {
    "sm": {
      "$value": [
        {
          "x": "0px",
          "y": "1px",
          "blur": "2px",
          "spread": "0px",
          "color": "rgb(0 0 0 / 5%)"
        }
      ],
      "$type": "shadow"
    },
    "md": {
      "$value": [
        {
          "x": "0px",
          "y": "4px",
          "blur": "6px",
          "spread": "0px",
          "color": "rgb(0 0 0 / 10%)"
        }
      ],
      "$type": "shadow"
    },
    "lg": {
      "$value": [
        {
          "x": "0px",
          "y": "10px",
          "blur": "15px",
          "spread": "0px",
          "color": "rgb(0 0 0 / 10%)"
        }
      ],
      "$type": "shadow"
    }
  }
}