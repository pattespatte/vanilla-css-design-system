    "family": {
      "base": {
        "$value": "system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, sans-serif",
        "$type": "string"
      }
    }
  },
  "text": {
    "sm": {
      "$value": "0.875rem",
      "$type": "dimension"
    },
    "lg": {
      "$value": "1.25rem",
      "$type": "dimension"
    },
    "light": {
      "$value": "200",
      "$type": "string"
    },
    "base": {
      "$value": "400",
      "$type": "string"
    },
    "bold": {
      "$value": "700",
      "$type": "string"
    }
  },
  "line": {
