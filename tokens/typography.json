{
  "font": {
    "family": {
      "base": {
        "$value": "\"Public Sans\", system-ui, -apple-system, blinkmacsystemfont, \"Segoe UI\",\n\t\troboto, oxygen, ubuntu, cantarell, sans-serif",
        "$type": "fontFamily"
      }
    }
  },
  "text": {
    "size": {
      "sm": {
        "$value": "0.875rem",
        "$type": "dimension"
      },
      "base": {
        "$value": "1rem",
        "$type": "dimension"
      },
      "lg": {
        "$value": "1.25rem",
        "$type": "dimension"
      }
    },
    "weight": {
      "light": {
        "$value": "200",
        "$type": "string"
      },
      "regular": {
        "$value": "400",
        "$type": "string"
      },
      "bold": {
        "$value": "700",
        "$type": "string"
      }
    }
  },
  "line": {
    "height": {
      "base": {
        "$value": 1.5,
        "$type": "number"
      }
    }
  }
}